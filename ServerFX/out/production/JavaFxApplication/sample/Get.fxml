<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="180.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GetController">
   <children>
      <TextField fx:id="idText" layoutX="25.0" layoutY="19.0" />
      <TextField fx:id="nameText" layoutX="25.0" layoutY="64.0" />
      <Button fx:id="getBtn" layoutX="82.0" layoutY="153.0" mnemonicParsing="false" text="Get" onAction="#getAction"/>
      <RadioButton fx:id="idRB" layoutX="14.0" layoutY="1.0" mnemonicParsing="false" onAction="#idRBAction" text="Get By Id" />
      <RadioButton fx:id="nameRB" layoutX="14.0" layoutY="45.0" mnemonicParsing="false" onAction="#nameRBAction" text="Get By Name" />
      <TextField fx:id="savedNameText" layoutX="25.0" layoutY="114.0" prefHeight="26.0" prefWidth="150.0" />
      <Label layoutX="13.0" layoutY="96.0" text="Name for downloaded file" />
   </children>
</AnchorPane>
