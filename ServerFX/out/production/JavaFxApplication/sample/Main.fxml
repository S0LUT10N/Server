<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="230.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainController">
   <children>
      <ImageView fitHeight="300.0" fitWidth="300.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../cartina.png" />
         </image>
      </ImageView>
      <Button fx:id="getBtn" layoutX="100.0" layoutY="26.0" mnemonicParsing="false" onAction="#getBtnAction" prefHeight="38.0" prefWidth="100.0" text="Get" />
      <Button fx:id="saveBtn" layoutX="100.0" layoutY="98.0" mnemonicParsing="false" onAction="#saveBtnAction" prefHeight="38.0" prefWidth="100.0" text="Save" />
      <Button fx:id="deleteBtn" layoutX="100.0" layoutY="171.0" mnemonicParsing="false" onAction="#deleteBtnAction" prefHeight="38.0" prefWidth="100.0" text="Delete" />
   </children>
</AnchorPane>
