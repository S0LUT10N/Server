<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="180.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DeleteController">
   <children>
      <RadioButton fx:id="idRB" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Delete by Id" onAction="#idRBAction"/>
      <RadioButton fx:id="nameRB" layoutX="14.0" layoutY="58.0" mnemonicParsing="false" text="Delete by Filename" onAction="#nameRBAction"/>
      <TextField fx:id="idText" layoutX="25.0" layoutY="32.0" />
      <TextField fx:id="nameText" layoutX="25.0" layoutY="77.0" />
      <Button fx:id="deleteBtn" layoutX="74.0" layoutY="129.0" mnemonicParsing="false" text="Delete" onAction="#deleteAction" />
   </children>
</AnchorPane>
