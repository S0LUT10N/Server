   <children>
      <TextField fx:id="fileNameText" layoutX="25.0" layoutY="32.0" />
      <TextField fx:id="fileServerNameText" layoutX="25.0" layoutY="77.0" />
      <Button fx:id="saveBtn" layoutX="79.0" layoutY="129.0" mnemonicParsing="false" text="Save" onAction="#saveAction" />
      <Label layoutX="14.0" layoutY="14.0" text="Name of the file" />
      <Label layoutX="14.0" layoutY="58.0" text="Name to be saved on server" />
   </children>
</AnchorPane>
